{
  "name": "@codehub/root-config",
  "type": "module",
  "scripts": {
    "start": "webpack serve --port 9000 --env isLocal",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "build": "webpack --mode=production --config webpack.config.cjs",
    "develop": "cross-env NODE_ENV='development' concurrently -n w: 'yarn:develop:*'",
    "develop:node": "nodemon -e js,ejs,html --experimental-loader @node-loader/core src/server.js",
    "develop:webpack": "webpack-dev-server --mode=development --port 9876 --env isLocal=true --config webpack.config.cjs",
    "start:node": "node --experimental-loader @node-loader/core server/server.js",
    "format": "prettier --write './**'"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-node-important-stuff": "^1.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-cli": "^27.0.5",
    "lru-cache": "^6.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "serve": "^12.0.0",
    "serve-favicon": "^2.5.0",
    "ts-config-single-spa": "^2.0.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-config-single-spa-ts": "^3.0.0",
    "webpack-dev-server": "^4.0.0-rc.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@node-loader/core": "^1.0.3",
    "@node-loader/http": "^1.0.1",
    "@node-loader/import-maps": "^1.0.3",
    "@types/jest": "^26.0.23",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.16.0",
    "axios": "^0.21.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "import-map-overrides": "^2.1.0",
    "merge2": "^1.4.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "single-spa": "^5.9.3",
    "single-spa-layout": "1.6.0",
    "single-spa-web-server-utils": "^1.17.0",
    "view-design": "^4.4.0",
    "vue": "^2.6.12",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
